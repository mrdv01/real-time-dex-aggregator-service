# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_TLS=false

# API Configuration
DEXSCREENER_BASE_URL=https://api.dexscreener.com
JUPITER_BASE_URL=https://api.jup.ag
GECKOTERMINAL_API_KEY=your_api_key_here
GECKOTERMINAL_BASE_URL=https://api.geckoterminal.com/api/v2

# Cache Configuration
CACHE_TTL=30
CACHE_ENABLED=true

# Rate Limiting (per minute)
RATE_LIMIT_DEXSCREENER_MAX=300
RATE_LIMIT_DEXSCREENER_WINDOW=60000
RATE_LIMIT_JUPITER_MAX=100
RATE_LIMIT_JUPITER_WINDOW=60000
RATE_LIMIT_GECKOTERMINAL_MAX=30
RATE_LIMIT_GECKOTERMINAL_WINDOW=60000

# WebSocket Configuration
WS_UPDATE_INTERVAL=10000
WS_PRICE_CHANGE_THRESHOLD=5
WS_VOLUME_SPIKE_MULTIPLIER=2

# Logging
LOG_LEVEL=info
